class Solution {
    public int lengthOfLastWord(String s) {
        String[] splitStr = s.split("\\s+");
        int count = 0; 
        for (int i=0 ; i<splitStr.length ; i++){
            if (i == (splitStr.length-1)){
                count = splitStr[i].length();
            }
        }
        return count;
    }
}
